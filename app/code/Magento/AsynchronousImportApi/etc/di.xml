<config xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:noNamespaceSchemaLocation="urn:magento:framework:ObjectManager/etc/config.xsd">
    <preference for="Magento\AsynchronousImportApi\Model\SourceValidatorInterface"
                type="Magento\AsynchronousImportApi\Model\SourceValidatorChain"/>
    <preference for="Magento\AsynchronousImportApi\Model\ImportValidatorInterface"
                type="Magento\AsynchronousImportApi\Model\ImportValidatorChain"/>
    <preference for="Magento\AsynchronousImportApi\Model\SourceReaderInterface"
                type="Magento\AsynchronousImportApi\Model\SourceReaderChain"/>
    <preference for="Magento\AsynchronousImportApi\Model\ConvertingRulesProcessorInterface"
                type="Magento\AsynchronousImportApi\Model\ConvertingRulesProcessorChain"/>
    <preference for="Magento\AsynchronousImportApi\Model\ImportDataProcessorInterface"
                type="Magento\AsynchronousImportApi\Model\ImportDataProcessorChain"/>

    <type name="Magento\AsynchronousImportApi\Model\ConvertingRulesProcessorInterface">
        <arguments>
            <argument name="convertingRules" xsi:type="array">
                <item name="leaveColumns" xsi:type="object">Magento\AsynchronousImportApi\Model\ProcessingRule\LeaveColumns</item>
            </argument>
        </arguments>
    </type>
</config>
